net: "patch_autoencoder.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 100
test_interval: 100000
test_compute_loss: true
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 100
max_iter: 100000
weight_decay: 0.0015
snapshot: 100000
snapshot_prefix: "autoencoder"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: GPU
